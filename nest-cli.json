{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/finance-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/finance-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/finance-app",
  "projects": {
    "finance-app": {
      "type": "application",
      "root": "apps/finance-app",
      "entryFile": "main",
      "sourceRoot": "apps/finance-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/finance-app/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "family-service": {
      "type": "application",
      "root": "apps/family-service",
      "entryFile": "main",
      "sourceRoot": "apps/family-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/family-service/tsconfig.app.json"
      }
    },
    "financial-service": {
      "type": "application",
      "root": "apps/financial-service",
      "entryFile": "main",
      "sourceRoot": "apps/financial-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/financial-service/tsconfig.app.json"
      }
    },
    "balance-service": {
      "type": "application",
      "root": "apps/balance-service",
      "entryFile": "main",
      "sourceRoot": "apps/balance-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/balance-service/tsconfig.app.json"
      }
    },
    "bank-service": {
      "type": "application",
      "root": "apps/bank-service",
      "entryFile": "main",
      "sourceRoot": "apps/bank-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bank-service/tsconfig.app.json"
      }
    },
    "ai-chat-service": {
      "type": "application",
      "root": "apps/ai-chat-service",
      "entryFile": "main",
      "sourceRoot": "apps/ai-chat-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ai-chat-service/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}